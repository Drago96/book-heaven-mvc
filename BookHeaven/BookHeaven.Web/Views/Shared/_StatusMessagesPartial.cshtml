@if (TempData.ContainsKey(DataKeyConstants.ErrorMessage))
{
    <div class="alert alert-danger text-center custom-alert fade">
        @TempData[DataKeyConstants.ErrorMessage]
    </div>
}
@if (TempData.ContainsKey(DataKeyConstants.SuccessMessage))
{
    <div class="alert alert-success text-center custom-alert fade">
        @TempData[DataKeyConstants.SuccessMessage]
    </div>
}
@if (TempData.ContainsKey(DataKeyConstants.WarningMessage))
{
    <div class="alert alert-warning text-center custom-alert fade">
        @TempData[DataKeyConstants.WarningMessage]
    </div>
}